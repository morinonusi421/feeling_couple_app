<% if @party.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains <%= pluralize(@party.errors.count, "error") %>.
    </div>
    <ul>
    <% @party.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>s
    <% end %>
    </ul>
  </div>
<% end %>
